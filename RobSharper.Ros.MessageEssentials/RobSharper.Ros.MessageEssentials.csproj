<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Guido Breitenhuber</Authors>
        <Copyright>JOANNEUM RESEARCH Forschungsgesellschaft mbH</Copyright>
        <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
        <Description>
            RobSharper.Ros.MessageEssentials contains 
            Attribute definitions for ROS Message/Service/Action POCOs,
            Formatters for serializing ROS messages,
            Helpers for retrieving ROS message MD5 hashes and ROS message definitions.
        </Description>
        <PackageTags>ROS;RobSharper;RobSharper.Ros;ROS.Messages;</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Antlr4.Runtime.Standard" Version="4.8.0" />
    </ItemGroup>

</Project>
